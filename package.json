{
	"name": "solid-system",
	"type": "module",
	"private": "true",
	"scripts": {
		"dev": "solid-start dev --port 3000",
		"start": "solid-start start --port 3000",
		"build": "solid-start build",
		"push": "prisma db push",
		"postinstall": "prisma generate",
		"prepare": "node npm.prepare.cjs",
		"rome:lint": "rome check .",
		"rome:lint:fix": "rome check . --apply-suggested",
		"rome:lint:fix:unsafe": "rome check . --apply-unsafe",
		"rome:format": "rome format .",
		"rome:format:write": "rome format . --write",
		"rome:ci": "rome ci .",
		"eslint:lint": "eslint \"**/*.{ts,tsx,js,jsx}\"",
		"eslint:fix": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
		"pre:commit": "lint-staged",
		"format": "prettier --write \"**/*.{tsx,jsx}\" && rome format --write .",
		"pg": "nodemon --watch \"./**/*.*\" --exec \"pnpm ts-node ./playground.ts\""
	},
	"dependencies": {
		"@auth/core": "^0.5.1",
		"@auth/solid-start": "^0.1.1",
		"@next-auth/prisma-adapter": "^1.0.5",
		"@prisma/client": "^4.12.0",
		"@solid-primitives/storage": "^1.3.9",
		"@solidjs/meta": "^0.28.4",
		"@solidjs/router": "^0.8.2",
		"@tanstack/solid-query": "^5.0.0-alpha.2",
		"@tanstack/solid-table": "^8.8.5",
		"@trpc/client": "^10.19.1",
		"@trpc/server": "^10.19.1",
		"solid-js": "^1.7.3",
		"solid-start": "^0.2.26",
		"solid-start-trpc": "^0.0.16",
		"solid-trpc": "0.1.0-sssr.7",
		"theme-change": "^2.5.0",
		"undici": "5.21.2",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@fontsource/fira-sans": "^4.5.10",
		"@swc/core": "^1.3.46",
		"@tailwindcss/typography": "^0.5.9",
		"@total-typescript/ts-reset": "^0.4.2",
		"@types/node": "^18.15.11",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"autoprefixer": "^10.4.14",
		"daisyui": "^2.51.5",
		"eslint": "^8.38.0",
		"eslint-plugin-solid": "^0.12.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"nodemon": "^2.0.22",
		"postcss": "^8.4.21",
		"prettier": "^2.8.7",
		"prettier-plugin-tailwindcss": "^0.2.7",
		"prisma": "^4.12.0",
		"rome": "^12.0.0",
		"solid-start-node": "^0.2.26",
		"solid-start-vercel": "^0.2.26",
		"tailwindcss": "^3.3.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^3.12.6",
		"typescript": "^5.0.4",
		"vite": "^4.2.1"
	},
	"engines": {
		"node": ">=18.15.0"
	},
	"packageManager": "pnpm@8.0.0",
	"volta": {
		"node": "18.15.0"
	}
}
